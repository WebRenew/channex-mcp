# Channex API Configuration
CHANNEX_API_KEY=your_channex_api_key_here
CHANNEX_BASE_URL=https://app.channex.io/api/v1/

# Server Configuration
PORT=3000
MCP_MODE=http # Options: http, mcp

# API Keys for HTTP mode (format: key:userId:name)
# Multiple keys separated by commas
API_KEYS=ck_prod_key1:user1:Production User,ck_prod_key2:user2:Development User

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Environment
NODE_ENV=production