{
  "name": "channex-mcp",
  "version": "1.0.0",
  "description": "Self-improving MCP server for Channex.io API integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "command": "tsx .claude/commands/run.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \".claude/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "channex",
    "api",
    "pms",
    "channel-manager"
  ],
  "author": "Supahost",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0"
  }
}
